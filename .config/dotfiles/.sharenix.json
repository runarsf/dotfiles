{
  "DefaultFileUploader": "xBackBone",
  "DefaultImageUploader": "xBackBone",
  "DefaultTextUploader": "Pastor",

  "Services": [
    {
      "Version": "13.6.1",
      "Name": "xBackBone",
      "DestinationType": "ImageUploader, TextUploader, FileUploader",
      "RequestType": "POST",
      "RequestURL": "https://i.runarsf.dev/upload",
      "FileFormName": "upload",
      "Body": "MultipartFormData",
      "Arguments": {
        "file": "$filename$",
        "text": "$input$",
        "token": "<++>"
      },
      "FileFormName": "upload",
      "URL": "$json:url$",
      "ThumbnailURL": "$json:url$/raw",
      "DeletionURL": "$json:url$/delete/<++>"
    },
    {
      "Version": "13.6.1",
      "Name": "Pastor",
      "DestinationType": "ImageUploader, TextUploader, FileUploader",
      "RequestMethod": "POST",
      "RequestURL": "https://p.runarsf.dev/",
      "Body": "MultipartFormData",
      "FileFormName": "file",
      "RegexList": [
        "^(https?:\\/\\/.+\\/)([^\\s]+)\\s(.*)$"
      ],
      "URL": "$regex:1|1$$regex:1|2$",
      "ThumbnailURL": "$regex:1|1$$regex:1|2$",
      "DeletionURL": "$regex:1|1$delete/$regex:1|2$?token=$regex:1|3$"
    },
    {
      "Name": "Chibisafe",
      "RequestType": "POST",
      "RequestURL": "https://i.runarsf.dev/api/upload",
      "FileFormName": "files[]",
      "Headers": {
        "token": "<++>"
      },
      "ResponseType": "Text",
      "URL": "$json:files[0].url$"
    }
  ]
}
