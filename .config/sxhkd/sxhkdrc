super + Return
	$TERMINAL
super + shift + q
	kill -9 `xdotool getwindowfocus getwindowpid`
super + x
	$HOME/bin/customlock.sh "gllock"
super + shift + x
	$HOME/bin/prompt.sh "Are you sure you want to shutdown?" "$shutdown"
super + v
	$TERMINAL -e nvim
super + d
	rofi -show run -lines 3 -eh 2
super + Tab
	rofi -show window
super + shift + w
	$BROWSER

alt + p
	sharenix-section -n -c
alt + shift + p
	$HOME/git/pyshare/pyshare-runner.py -s -u -c -d


ctrl + @comma
	xdotool keyup ctrl && xdotool key less && xdotool keydown ctrl
ctrl + @period
	xdotool keyup ctrl && xdotool key greater && xdotool keydown ctrl

