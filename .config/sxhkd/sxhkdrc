super + x
	$HOME/bin/i3lock-custom.sh
super + shift + x
	$HOME/bin/prompt.sh "Are you sure you want to shutdown?" "$shutdown"

alt + p
	sharenix-section -n -c
alt + shift + p
	$HOME/git/pyshare/pyshare-runner.py -s -u -c -d

super + Return
	exec $TERMINAL
super + shift + q
	xkill

super + d
	rofi -show run -lines 3 -eh 2
super + Tab
	rofi -show window


