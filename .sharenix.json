{
  "DefaultFileUploader": "url.tld",
  "DefaultImageUploader": "url.tld",

  "Services": [
      {
         "Name": "url.tld",
         "RequestType": "POST",
         "RequestURL": "https://url.tld/api/upload",
	 "FileFormName": "files[]",
	 "Headers": {
	      "token": $sharenix_token
	 },
	 "ResponseType": "Text",
	 "URL": "$json:files[0].url$"
      }
  ]
}
